local DiscordLib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/discord%20lib.txt")()



local win = DiscordLib:Window("antioderhub loader")



local serv = win:Server("Loader", "")



local drops = serv:Channel("Dropdowns")





local drop = drops:Dropdown("Select your language:",{"Русский","English"}, function(bool)

print(bool)

end)



drops:Button("Clear", function()

drop:Clear()

end)



drops:Button("Add option", function()loadstring(game:HttpGet("https://raw.githubusercontent.com/aftermath-dev/aftermath-hubs/refs/heads/main/antioderhub"))()

drop:Add("Russian")

end)



serv:Channel("by aftermath")
