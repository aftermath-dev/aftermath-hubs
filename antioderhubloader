local DiscordLib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/discord%20lib.txt")()



local win = DiscordLib:Window("antioderhub loader")



local serv = win:Server("Loader", "")



local btns = serv:Channel("Универсал. скрипты")

btns:Button("ESP", function() loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-Esp-open-source-15582"))()
DiscordLib:Notification("Уведомление", "Активировано!", "Ок!")
end)

btns:Seperator()

btns:Button("NoClip", function() loadstring(game:HttpGet("https://pastebin.com/raw/L6Bi4MrA"))()
DiscordLib:Notification("Уведомление", "Активировано!", "Ок!")
end) 



drops:Button("Add option", function()loadstring(game:HttpGet("https://raw.githubusercontent.com/aftermath-dev/aftermath-hubs/refs/heads/main/antioderhubv2"))()

end)



serv:Channel("by aftermath")
